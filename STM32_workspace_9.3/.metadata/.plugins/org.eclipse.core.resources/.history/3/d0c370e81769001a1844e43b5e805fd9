#include<stdio.h>

int and_bit_by_bit(int param_A, int param_B) {
	int result;
	if (param_A && param_B) {
		result = 1;
	} else {
		result = 0;
	}
	return result;
}

int main(void) {
	int result_bit_by_bit;
	int param_A = 0;
	int param_B = 0;
	result_bit_by_bit = and_bit_by_bit(param_A, param_B);


	printf("%i", result_bit_by_bit);

	return 0;
}
